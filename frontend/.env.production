# API Configuration
VITE_API_BASE_URL=https://api.yourdomain.com/api
VITE_ADMIN_URL=https://api.yourdomain.com/admin

# Application Configuration
VITE_APP_NAME=技能集市
VITE_APP_DESCRIPTION=中文技能服务与交易平台
VITE_APP_VERSION=1.0.0

# Environment
VITE_NODE_ENV=production

# Feature Flags
VITE_ENABLE_SOCIAL_LOGIN=true
VITE_ENABLE_WECHAT_PAY=true
VITE_ENABLE_ALIPAY=true

# External Services
VITE_WECHAT_APP_ID=your_production_wechat_app_id
VITE_QQ_APP_ID=your_production_qq_app_id

# Analytics (optional)
VITE_ENABLE_ANALYTICS=true
VITE_GA_TRACKING_ID=your_ga_tracking_id

# Debug mode
VITE_DEBUG=false